{
  "db_name": "PostgreSQL",
  "query": "\nINSERT INTO jobs (kind, payload, status, next_run, continuation)\n     VALUES ($1, $2, 'queued', $3, $4)\nON CONFLICT (kind, payload) DO UPDATE\n        SET status       = 'queued',\n            next_run     = $3,\n            continuation = $4,\n            updated_at   = NOW()\n  RETURNING job_id\n            ",
